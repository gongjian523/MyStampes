<Window x:Class="MyStampes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MyStampes" Height="500" Width="1000">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="110"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Orientation="Vertical" VerticalAlignment="Top" Grid.Column="0">
            <Button Width="100" Margin="0,5,0,0" Click="EnterLog">日志</Button>
            <Button Width="100" Margin="0,5,0,0" Click="EnterAddrBook">通讯录</Button>
        </StackPanel>
        <StackPanel Grid.Column="1">
            <Frame x:Name="ContentFrame" NavigationUIVisibility="Hidden" />
        </StackPanel>
    </Grid>
</Window>
