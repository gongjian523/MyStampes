<Window x:Class="MyStampes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Name="MianWin"
        Title="MyStampes"
        Height="700"
        MinHeight="700" 
        Width="1000"
        MinWidth ="1000">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="48"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Orientation="Vertical" VerticalAlignment="Top" Grid.Column="0">
            <Button Width="48" Height="48" Click="EnterLogList">日志</Button>
            <Button Width="48" Height="48"  Click="EnterAddrBook">通讯录</Button>
        </StackPanel>
        <StackPanel Grid.Column="1">
            <Frame x:Name="ContentFrame" NavigationUIVisibility="Hidden" />
        </StackPanel>
    </Grid>
</Window>
